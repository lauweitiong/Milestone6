   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "arr = []\n",
    "for file in os.listdir(r\"https://github.com/lauweitiong/Milestone6/tree/master/Data"):\n",
    "    if file.endswith(\".pdf\"):\n",
    "        arr.append(file)\n",
    "\n",
    "filenames = [f for f in arr] "
   ]
